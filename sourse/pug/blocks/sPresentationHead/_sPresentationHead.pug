mixin sPresentationHead(data)
	// start sPresentationHead
	+b.sPresentationHead#sPresentationHead&attributes(attributes)
		+e.inner.bg-wrap
			picture.picture-bg
				source(srcset=`img/@2x/webp/${data.img}.webp`   type='image/webp')
				img(src=`img/@2x/${data.img}.png` alt='')

			+b.canvas-block
				
				.container-fluid
					+e.block--logo
						+pic("logo-block.png",'',0)
					.row
						.col-auto 
							+e.block--aside.--left
								+e.count 150
								+e.stand
								+pic("left-banner.png",'',0)
						.col
							+e.block--video-banner
								script(src='https://broadcast.comdi.com/player/js/embed.min.js', data-comdi-event='r5ftb9xd', data-login='anonymous', data-registration='{"email":"anonymous@startupvillage.ru"}', lang='ru', async='')
						.col-auto
							+e.block--aside.--slider
								+e.slider--js.swiper-container
									+arrow
									.swiper-wrapper
										- for (var x = 0; x < 3; x++)
											+e.slide.swiper-slide
												+e.caption
													+e.name!= `Виктор <br> Барышников ${x + 1}`
													+e.prof Product owner
												+e.slide-img-wrap
													img.swiper-lazy(src=`img/@2x/speaker-${x + 1}.png`, alt="")
												+e.btn-title(href="#") Начать <br> чат
					+e.block--btn
						mixin btn(i,t,img)
							+e.btn(href="#")&attributes(attributes)
								+e.icon-wrap
									if i 
										+icon(`btn-`+ i)
									else
										img(src=`img/svg/btn-1.svg`, alt="")
									
								+e.btn-title!= t
								block
						+btn('',`Участвовать <br> в розыгрыше`)
						+btn('2',`Скачать <br> материалы`) 
						+btn('4',`Войти <br> в демо-зону`)(class="disabled") 
						+btn('3',`Начать видеозвонок <br> с вендором`) 
							+e.caption
								+e.title Я&nbsp;пока занят, напишите пожалйста в&nbsp;чат
				+e.btn-bottom
					<svg width="360" height="195" viewBox="0 0 360 195" fill="none" xmlns="http://www.w3.org/2000/svg">
						<path d="M62.9141 74L179 99.9752L299 74" stroke="#FF5700"/>
						<path d="M62.9141 84.0144L179 109.99L299 84.0144" stroke="#FF5700"/>
						<path d="M62.9141 94.7803L179 120.755L299 94.7803" stroke="#FF5700"/>
						</svg>
	// end sPresentationHead